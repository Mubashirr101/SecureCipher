    color: #737373; /* White */
}


/* Generate key button */
#generate_key_button {
    background-color: #FFFFFF; /* White */
    color: #000000; /* Black */
    border-radius: 9px;
    border: 3px solid #324C5C;

}
#generate_key_button:hover {
    background-color: #507A94; /* Black */
    border-radius: 9px;
    border: 1px solid #000000;
